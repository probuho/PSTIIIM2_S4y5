{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && npx tsc",
    "start": "node dist/index.js",
    "dev": "cross-env NODE_ENV=development npm run build && concurrently \"npx tsc -w\" \"nodemon --exec ts-node src/index.ts\"",
    "dev:staging": "cross-env NODE_ENV=staging npm run build && concurrently \"npx tsc -w\" \"nodemon --exec ts-node src/index.ts\"",
    "dev:prod": "cross-env NODE_ENV=production npm run build && concurrently \"npx tsc -w\" \"nodemon --exec ts-node src/index.ts\"",
    "seed-scores": "tsx src/seed-scores.ts",
    "seed-scores:staging": "NODE_ENV=staging tsx src/seed-scores.ts",
    "seed-scores:prod": "NODE_ENV=production tsx src/seed-scores.ts",
    "seed-scores:force": "tsx src/seed-scores.ts --force",
    "test:config": "tsx -e \"import { validateConfig } from './src/config/environments'; validateConfig();\"",
    "setup:dev": "node setup-env.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/leaflet": "^1.9.19",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.15.17",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^8.0.0",
    "prisma": "^6.7.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@auth/core": "^0.40.0",
    "@auth/express": "^0.11.0",
    "@prisma/client": "^6.7.0",
    "bcryptjs": "^3.0.2",
    "body-parser": "^2.2.0",
    "concurrently": "^9.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.524.0",
    "morgan": "^1.10.0",
    "react-router-dom": "^7.6.2",
    "rimraf": "^6.0.1",
    "zod": "^4.0.10"
  }
}
